generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model municipio_acessos {
  id           Int         @id(map: "MunicipioAcesso_pkey") @default(autoincrement())
  user_id      Int?
  municipio_id Int?
  exclusive    Boolean     @default(false)
  granted_at   DateTime?   @default(now()) @db.Timestamptz(6)
  valid_until  DateTime?   @db.Timestamptz(6)
  created_at   DateTime    @default(now()) @db.Timestamptz(6)
  updated_at   DateTime    @default(now()) @db.Timestamptz(6)
  uf           String?
  municipios   municipios? @relation(fields: [municipio_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "MunicipioAcesso_municipio_id_fkey")
  users        users?      @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "MunicipioAcesso_user_id_fkey")
}

model municipios {
  id                Int                 @id(map: "Municipio_pkey") @default(autoincrement())
  municipio         String
  name_state        String
  created_at        DateTime            @default(now()) @db.Timestamptz(6)
  updated_at        DateTime            @default(now()) @db.Timestamptz(6)
  municipio_acessos municipio_acessos[]

  @@unique([municipio, name_state], map: "Municipio_municipio_name_state_key")
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model spatial_ref_sys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model users {
  id                Int                 @id(map: "User_pkey") @default(autoincrement())
  email             String?             @unique(map: "User_email_key")
  hash              String
  role              String?
  username          String?             @unique(map: "User_username_key")
  name              String?
  cargo             String?
  photo             String?             @db.Text
  created_at        DateTime            @default(now()) @db.Timestamptz(6)
  updated_at        DateTime            @default(now()) @db.Timestamptz(6)
  municipio_acessos municipio_acessos[]
}

/// Tabela para gerenciar relacionamentos com munic√≠pios
model municipios_com_relacionamento {
  row_index              Int       @id @default(autoincrement())
  name_state             String    @db.VarChar(2)
  code_muni              String    @db.VarChar(10)
  name_muni              String    @db.VarChar(255)
  relacionamento_ativo   Boolean   @default(true)
  relacionamento_criado  DateTime? @db.Timestamptz(6)
  relacionamento_editado DateTime? @db.Timestamptz(6)

  @@unique([code_muni], map: "municipios_com_relacionamento_code_muni_key")
  @@map("municipios_com_relacionamento")
}
